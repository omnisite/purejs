<script id="main" type="text/template">
  <div class="navbar-collapse collapse">
  </div>
</script>
<script id="header" type="text/template">
  <div class="navbar-header" data-parent="#{{#def.id}}" href="#{{=it.id}}">
    <a class="navbar-brand" href="#">{{=it.name}}</a>
  </div>
</script>
<script id="toggle" type="text/template">
  <button type="button" class="navbar-toggle">
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
    <span class="icon-bar"></span>
  </button>
</script>
<script id="item" type="text/template">
  <li {{? it.active }} class="active"{{?}}>
    <a href="#">{{=it.name}}</a>
  </li>
</script>
<script id="dropdown" type="text/template">
  <li class="dropdown">
    <a class="dropdown-toggle" data-toggle="dropdown" href="#{{=it.id}}">
      {{=it.name}}
      <span class="caret"></span>
    </a>
  </li>
</script>
<script id="style" type="text/template">
  body {
    padding-top: 60px;
  }
  body > .navbar {
    position: fixed; top: 0; left: 0; width: 100%; z-index: 1000;
  }
  body > .navbar .dropdown.navbar-nav {
    width: 100%;
  }
  body > .navbar .navbar-toggle {
    position: absolute; top: 0; right: 0; margin-top: 6px; margin-right: 10px;
  }
  @media (max-width: 768px) {
    body > .navbar .navbar-collapse.collapse .pull-right {
      position: absolute; top: 40px; right: -20px; margin-right: 0;
    }
  }
</script>
