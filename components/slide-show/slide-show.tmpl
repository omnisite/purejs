<script id="slide" type="text/template">
    <div id="{{#def.id}}" class="drag-and-drop draggable{{? it.class }} {{=it.class}}{{?}}"></div>
</script>
<script id="test-body" type="text/template">
  <div id="myCarousel1" class="carousel slide carousel-fit" data-ride="carousel" data-interval="60000" style="height: 300px; margin-top: 324.5px;">
    
    <div class="carousel-inner">
      <div class="item">
        <img data-src="holder.js/900x500/auto/#777:#555/text:First slide" alt="First slide" src="data:image/svg+xml;base64,">
        <div class="carousel-caption" style="left: 45px; right: 45px;">
          <h3>First slide label</h3>
          <p>Nulla vitae elit libero, a pharetra augue mollis interdum.</p>
        </div>
      </div>
      <div class="item">
        <img data-src="holder.js/750x400/auto/#666:#444/text:Second slide" alt="Second slide" src="data:image/svg+xml;base64,">
        <div class="carousel-caption" style="left: 45px; right: 45px;">
          <h3>Second slide label</h3>
          <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
        </div>
      </div>
      <div class="item active">
        <img data-src="holder.js/600x300/auto/#555:#333/text:Third slide" alt="Third slide" src="data:image/svg+xml;base64,">
        <div class="carousel-caption" style="left: 45px; right: 45px;">
          <h3>Third slide label</h3>
          <p>Praesent commodo cursus magna, vel scelerisque nisl consectetur.</p>
        </div>
      </div>
    </div>
  </div>
</script>
<script id="top-button">
  <button class="btn btn-danger pull-right" type="button" data-dismiss="modal" aria-hidden="true">
    <span class="glyphicon glyphicon-remove"></span>
    <span>Close</span>
  </button>
</script>
<script id="control" type="text/template">
  <a class="{{=it.direction}} carousel-control" href="#{{#def.id}}" data-slide="{{=it.slide}}">
    <span class="glyphicon glyphicon-chevron-{{=it.direction}}"></span>
  </a>
</script>
<script id="fullscreen" data-type="style" type="text/template">
  .modal.modal-fullscreen .modal-content {
    background: rgba(0, 0, 0, .5);
  }
  .modal.modal-fullscreen .modal-header {
    border: 0;
    color: #fff;
  }
  .modal.modal-fullscreen .modal-footer {
    border-color: rgba(0, 0, 0, .5);
  }
</script>
<script id="caption" data-type="style" type="text/template">
  .modal.modal-fullscreen .modal-content {
    background: rgba(0, 0, 0, .5);
  }
  .modal.modal-fullscreen .modal-header {
    border: 0;
    color: #fff;
  }
  .modal.modal-fullscreen .modal-footer {
    border-color: rgba(0, 0, 0, .5);
  }
  #{{#def.id}} .carousel-caption {
    background-color: rgba(0, 0, 0, .5);
    bottom: -100%;
    -webkit-transition: bottom 1s;
    transition: bottom 1s;
  }
  #{{#def.id}} .item.active >.carousel-caption {
    bottom: 0;
  }
</script>